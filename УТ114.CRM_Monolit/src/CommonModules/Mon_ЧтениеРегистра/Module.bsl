		//Mon_Сервис.ЗаписатьВЛогМассивОшибок(СтруктураРеквизитовДокумента);
		
		Если ЭтоЗаказ И НЕ ТипДокументаЗагрузки = "ПеремещениеТоваров" И НЕ СтруктураРеквизитовДокумента.ЕстьКритическиеОшибки Тогда
			
			РеализацияСсылка = ВернутьРеализациюТоваровИУслуг(НовыйДокумент.Ссылка);
			
			Если РеализацияСсылка = Документы.РеализацияТоваровУслуг.ПустаяСсылка() Тогда 
			
		 		ДокОбъект = Документы.РеализацияТоваровУслуг.СоздатьДокумент();
			Иначе
				ДокОбъект = РеализацияСсылка.ПолучитьОбъект();
			КонецЕсли;
			
			ДокОбъект.ПометкаУдаления = Ложь;
			ДокОбъект.Товары.Очистить();
			
			ДокОбъект.Заполнить(НовыйДокумент.Ссылка);
			
			Попытка
				НоменклатураСервер.ЗаполнитьСерииПоFEFO( ДокОбъект, Документы.РеализацияТоваровУслуг.ПараметрыУказанияСерий( ДокОбъект ) );
			Исключение
				
				СтруктураРеквизитовДокумента.ЕстьКритическиеОшибки = Истина;
				а = ОписаниеОшибки();
				ДокОбъект.Комментарий = ДокОбъект.Комментарий + ". Не удалось заполнить серии по FEFO";
				
			КонецПопытки;
			ДокОбъект.СкидкиРассчитаны = Истина;
			ДокОбъект.Менеджер = НовыйДокумент.Менеджер;
			ДокОбъект.Дата = СтруктураРеквизитовДокумента.ДатаОтгрузки + ТекущееВремя;
			ДокОбъект.ЗаполнитьЭтапыГрафикаОплаты();
			ДокОбъект.СостояниеЗаполненияМногооборотнойТары = Перечисления.СостоянияЗаполненияМногооборотнойТары.Заполнена;
			Попытка
				ДокОбъект.Записать(РежимЗаписиДокумента.Проведение);
				
			Исключение
				
				Если ЭтоЗаказ = Истина Тогда
					ДокОбъект.Серии.Очистить();
				    Для каждого Строка Из ДокОбъект.Товары Цикл
				       	Если Строка.СтатусУказанияСерий = 0 Тогда
							Продолжить;
				    	Иначе
				          	Строка.СтатусУказанияСерий = 5;  
				        КонецЕсли;      
					КонецЦикла;
				КонецЕсли;
				ДокОбъект.Записать(РежимЗаписиДокумента.Запись);
				НовыйДокумент.Записать(РежимЗаписиДокумента.Запись);
			КонецПопытки;
		ИначеЕсли ЭтоВозврат И
		СоответствиеУтилизационныхСкладов.Получить(СтруктураРеквизитовДокумента.Склад) = Истина И  
		(СтруктураРеквизитовДокумента.ЕстьКритическиеОшибки = Ложь И ГлобальныйРезультатПроверкиВозврата = Истина И РезультатПроверкиВозврата1с = Истина ) Тогда
		
			ДокОбъект = Документы.РеализацияТоваровУслуг.СоздатьДокумент();
			//вынести договор утилизации за пределы.
			ДокОбъект.ПометкаУдаления = Ложь;
			ДокОбъект.Организация = СтруктураШлюза.ШлюзОрганизации.ОрганизацияФ2;
			
			ДокОбъект.Комментарий = СтруктураРеквизитовДокумента.НомерЗаказа; 
			ДокОбъект.Товары.Очистить();
			ДокОбъект.Заполнить(НовыйДокумент.Ссылка);
			
			Для Каждого Строка ИЗ ДокОбъект.Товары Цикл
				Если НЕ Строка.Номенклатура.ТипНоменклатуры = Перечисления.ТипыНоменклатуры.МногооборотнаяТара Тогда
					Строка.СтатусУказанияСерий = 6;
				КонецЕсли;
			КонецЦикла;
			Для каждого Строка Из ДокОбъект.Серии Цикл
		       Строка.Склад = НовыйДокумент.Склад;      
			КонецЦикла;
			
			Если Ложь Тогда
			Попытка
				НоменклатураСервер.ЗаполнитьСерииПоFEFO( ДокОбъект, Документы.РеализацияТоваровУслуг.ПараметрыУказанияСерий( ДокОбъект ) );
			Исключение
				Для Каждого Строка ИЗ ДокОбъект.Товары Цикл
					Если НЕ Строка.Номенклатура.ТипНоменклатуры = Перечисления.ТипыНоменклатуры.МногооборотнаяТара Тогда
						Строка.СтатусУказанияСерий = 6;
					КонецЕсли;
				КонецЦикла;
				Для каждого Строка Из НовыйДокумент.Серии Цикл
			       СтрокаДобавленияСерии = ДокОбъект.Серии.Добавить();
			       ЗаполнитьЗначенияСвойств(СтрокаДобавленияСерии, Строка);      
				КонецЦикла;

				СтруктураРеквизитовДокумента.ЕстьКритическиеОшибки = Истина;
				а = ОписаниеОшибки();
				ДокОбъект.Комментарий = ДокОбъект.Комментарий + ". Не удалось заполнить серии по FEFO";
				
			КонецПопытки;
			КонецЕсли;
						
			ДокОбъект.Договор = Справочники.ДоговорыКонтрагентов.НайтиПоНаименованию("Договор утилизации");
			ДокОбъект.Соглашение = Справочники.СоглашенияСКлиентами.НайтиПоНаименованию("Балтика Утилизация");
			ДокОбъект.Контрагент = ДокОбъект.Договор.Контрагент;
			ДокОбъект.Партнер   = ДокОбъект.Договор.Партнер;
			ДокОбъект.СкидкиРассчитаны = Истина;
			ДокОбъект.Менеджер = НовыйДокумент.Менеджер;
			ДокОбъект.Дата = НовыйДокумент.Дата + 1;
			ДокОбъект.ЗаполнитьЭтапыГрафикаОплаты();
			//ДокОбъект.Серии.Очистить();
			Если ДокОбъект.Соглашение <> Справочники.СоглашенияСКлиентами.ПустаяСсылка() И ДокОбъект.Договор <> Справочники.ДоговорыКонтрагентов.ПустаяСсылка() Тогда
		      	ДокОбъект.ЗаполнитьУсловияПродажПоСоглашению();
			КонецЕсли;
			Попытка
				ДокОбъект.Записать(РежимЗаписиДокумента.Проведение);
			Исключение
				
				а = ОписаниеОшибки();
				ДокОбъект.Комментарий = ДокОбъект.Комментарий + а;
				ДокОбъект.Записать(РежимЗаписиДокумента.Запись);
			КонецПопытки;
				
		КонецЕсли;		
	КонецЦикла;
	
КонецПроцедуры

#Область ВспомогательныеПроцедуры
Функция ВернутьРезультатПроверкиВозврата(LineNumber, НомерЗаказа, ДатаЗаказа, СписокПримечаний)
	
	Результат = Истина;
	ИндексНайденнойСтроки = 0;
	Пока ИндексНайденнойСтроки <= СписокПримечаний.Вграница() Цикл
		
		Если СписокПримечаний[ИндексНайденнойСтроки].OptionTypeId = "ReturnChecked" 
			И СписокПримечаний[ИндексНайденнойСтроки].CRMOrderDate = ДатаЗаказа 
			И СписокПримечаний[ИндексНайденнойСтроки].CRMOrderNumber =  НомерЗаказа 
			И СписокПримечаний[ИндексНайденнойСтроки].LineNumber = LineNumber Тогда 
			Результат = (СписокПримечаний[ИндексНайденнойСтроки].Value = "1");	
			Возврат Результат;
		КонецЕсли;
		
		ИндексНайденнойСтроки = ИндексНайденнойСтроки + 1;
	КонецЦикла;
	
	Возврат Результат;

КонецФункции

Функция ВернутьЦенуИзПрайса(Номенклатура, Соглашение)
	
	Цена = 0;
	
	Запрос = Новый Запрос;
	Запрос.Текст = "ВЫБРАТЬ
		|	СоглашенияСКлиентамиЦеновыеГруппы.ЦеноваяГруппа КАК ЦеноваяГруппа,
		|	СоглашенияСКлиентамиЦеновыеГруппы.ВидЦен КАК ВидЦен
		|ПОМЕСТИТЬ ВТ_Прайсы
		|ИЗ
		|	Справочник.СоглашенияСКлиентами.ЦеновыеГруппы КАК СоглашенияСКлиентамиЦеновыеГруппы
		|ГДЕ
		|	СоглашенияСКлиентамиЦеновыеГруппы.Ссылка = &Соглашение
		|
		|ОБЪЕДИНИТЬ ВСЕ
		|
		|ВЫБРАТЬ
		|	NULL,
		|	СоглашенияСКлиентами.ВидЦен
		|ИЗ
		|	Справочник.СоглашенияСКлиентами КАК СоглашенияСКлиентами
		|ГДЕ
		|	СоглашенияСКлиентами.Ссылка = &Соглашение
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	Номенклатура.Ссылка КАК Ссылка,
		|	Номенклатура.ЦеноваяГруппа КАК ЦеноваяГруппа
		|ПОМЕСТИТЬ ВТ_Номенклатура
		|ИЗ
		|	Справочник.Номенклатура КАК Номенклатура
		|ГДЕ
		|	Номенклатура.Ссылка = &Номенклатура
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ВТ_Номенклатура.Ссылка КАК Номенклатура,
		|	ВТ_Прайсы.ВидЦен КАК ВидЦен
		|ПОМЕСТИТЬ ВТ_НоменклатураВидЦен
		|ИЗ
		|	ВТ_Номенклатура КАК ВТ_Номенклатура
		|		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Прайсы КАК ВТ_Прайсы
		|		ПО ВТ_Номенклатура.ЦеноваяГруппа = ВТ_Прайсы.ЦеноваяГруппа
		|		ИЛИ НЕ ВТ_Прайсы.ВидЦен = ЗНАЧЕНИЕ(Справочник.ВидыЦен.ПустаяСсылка)
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ЦеныНоменклатурыСрезПоследних.Период КАК Период,
		|	ЦеныНоменклатурыСрезПоследних.Номенклатура КАК Номенклатура,
		|	ЦеныНоменклатурыСрезПоследних.ВидЦены КАК ВидЦены,
		|	ЦеныНоменклатурыСрезПоследних.Цена КАК Цена,
		|	ЦеныНоменклатурыСрезПоследних.Валюта КАК Валюта
		|ИЗ
		|	РегистрСведений.ЦеныНоменклатуры.СрезПоследних(, (Номенклатура, ВидЦены) В
		|		(ВЫБРАТЬ
		|			ВТ_НоменклатураВидЦен.Номенклатура,
		|			ВТ_НоменклатураВидЦен.ВидЦен
		|		ИЗ
		|			ВТ_НоменклатураВидЦен КАК ВТ_НоменклатураВидЦен)) КАК ЦеныНоменклатурыСрезПоследних";
	Запрос.УстановитьПараметр("Номенклатура", Номенклатура);
	Запрос.УстановитьПараметр("Соглашение",   Соглашение);
	Выборка = Запрос.Выполнить().Выбрать();
	
	Если Выборка.Следующий() Тогда
		
		Цена = Выборка.Цена;
	
	КонецЕсли;
	
	Возврат Цена;		
		
КонецФункции

