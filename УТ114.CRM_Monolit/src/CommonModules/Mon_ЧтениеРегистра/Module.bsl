Функция ВернутьЦенуПродажиИзРеализации(Документ, Номенклатура, ЦенаИзМонолита)
	
	НайденнаяСтрока = Документ.Товары.Найти(Номенклатура,"Номенклатура");
	Если НайденнаяСтрока = Неопределено Тогда
	    ЦенаТовара = ЦенаИзМонолита;
	Иначе
	    ЦенаТовара = НайденнаяСтрока.Цена;
	КонецЕсли;
	Возврат ЦенаТовара;
	
КонецФункции

Функция ВернутьТаблицуЗначенийРеализацийСерийИДокументовРеализаций(ПартнерДляДокумента, Номенклатура, СрокГодностиДатой, ОрганизацияДляПоискаРеализаций, Шлюз)
	
	Запрос = Новый Запрос;
	Если Номенклатура.ТипНоменклатуры = Перечисления.ТипыНоменклатуры.МногооборотнаяТара Тогда
		
		Запрос.Текст = "ВЫБРАТЬ
			|	ПереданнаяВозвратнаяТараОстатки.Номенклатура КАК Номенклатура,
			|	1 КАК Приоритет,
			|	ПереданнаяВозвратнаяТараОстатки.ДокументПередачи КАК Документ,
			|	ПереданнаяВозвратнаяТараОстатки.ДокументПередачи.Организация КАК Организация,
			|	ПереданнаяВозвратнаяТараОстатки.КоличествоОстаток КАК КоличествоОборот
			|ИЗ
			|	РегистрНакопления.ПереданнаяВозвратнаяТара.Остатки(, Номенклатура = &Номенклатура
			|	И Партнер = &Получатель
			|	И (ДокументПередачи.Организация = &ОрганизацияДляПоискаРеализаций ИЛИ &ОрганизацияДляПоискаРеализаций = """")) КАК ПереданнаяВозвратнаяТараОстатки
			|УПОРЯДОЧИТЬ ПО
			|	ПереданнаяВозвратнаяТараОстатки.ДокументПередачи.Дата";
			
	Иначе
	
		Запрос.Текст = "ВЫБРАТЬ
			|	ДвиженияСерийТоваровОбороты.Номенклатура КАК Номенклатура,
			|	ДвиженияСерийТоваровОбороты.Серия КАК Серия,
			|	ДвиженияСерийТоваровОбороты.Документ КАК Документ,
			|	ВЫРАЗИТЬ(ДвиженияСерийТоваровОбороты.Документ КАК Документ.РеализацияТоваровУслуг).Дата КАК Период,
			|	ВЫРАЗИТЬ(ДвиженияСерийТоваровОбороты.Документ КАК Документ.РеализацияТоваровУслуг).Организация КАК Организация,
			|	ДвиженияСерийТоваровОбороты.КоличествоОборот КАК КоличествоОборот
			|ПОМЕСТИТЬ ВТ_ДокументыРеализации
			|ИЗ
			|	РегистрНакопления.ДвиженияСерийТоваров.Обороты(,, Запись, Номенклатура = &Номенклатура
			|	И Получатель = &Получатель
			|	И Документ ССЫЛКА Документ.РеализацияТоваровУслуг
			|	И (Серия.ГоденДо = &ГоденДо
			|	ИЛИ &ГоденДо = ДАТАВРЕМЯ(1, 1, 1))) КАК ДвиженияСерийТоваровОбороты
			|;
			|
			|////////////////////////////////////////////////////////////////////////////////
			|ВЫБРАТЬ
			|	ВТ_ДокументыРеализации.Номенклатура КАК Номенклатура,
			|	ВЫБОР
			|		КОГДА ВТ_ДокументыРеализации.Организация = &ОрганизацияДляПоискаРеализаций
			|			ТОГДА 1
			|		ИНАЧЕ 2
			|	КОНЕЦ КАК Приоритет,
			|	ВТ_ДокументыРеализации.Организация КАК Организация,
			|	ВТ_ДокументыРеализации.Серия КАК Серия,
			|	ВТ_ДокументыРеализации.Документ КАК Документ,
			|	ВТ_ДокументыРеализации.Период КАК Период,
			|	ВТ_ДокументыРеализации.КоличествоОборот - ЕСТЬNULL(ВозвратТоваровОтКлиентаТовары.Количество, 0) КАК КоличествоОборот
			|ИЗ
			|	ВТ_ДокументыРеализации КАК ВТ_ДокументыРеализации
			|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ВозвратТоваровОтКлиента.Серии КАК ВозвратТоваровОтКлиентаТовары
			|		ПО ВозвратТоваровОтКлиентаТовары.Номенклатура = ВТ_ДокументыРеализации.Номенклатура
			|		И ВозвратТоваровОтКлиентаТовары.Ссылка.Организация = ВТ_ДокументыРеализации.Организация
			|		И ВозвратТоваровОтКлиентаТовары.Серия = ВТ_ДокументыРеализации.Серия
			|		И ВозвратТоваровОтКлиентаТовары.Номенклатура = &Номенклатура
			|		И ВозвратТоваровОтКлиентаТовары.Ссылка.Партнер = &Получатель
			|		И ВозвратТоваровОтКлиентаТовары.Ссылка.Проведен
			|
			|ОБЪЕДИНИТЬ ВСЕ
			|
			|ВЫБРАТЬ ПЕРВЫЕ 1
			|	ДвиженияСерийТоваровОбороты.Номенклатура КАК Номенклатура,
			|	3,
			|	ВЫБОР
			|		КОГДА ДвиженияСерийТоваровОбороты.Документ ССЫЛКА Документ.РеализацияТоваровУслуг
			|			ТОГДА ВЫРАЗИТЬ(ДвиженияСерийТоваровОбороты.Документ КАК Документ.РеализацияТоваровУслуг).Организация
			|		КОГДА ДвиженияСерийТоваровОбороты.Документ ССЫЛКА Документ.ПриобретениеТоваровУслуг
			|			ТОГДА ВЫРАЗИТЬ(ДвиженияСерийТоваровОбороты.Документ КАК Документ.ПриобретениеТоваровУслуг).Организация
			|		ИНАЧЕ &ОрганизацияДляПоискаРеализацийФ2
			|	КОНЕЦ КАК Организация,
			|	ДвиженияСерийТоваровОбороты.Серия КАК Серия,
			|	ДвиженияСерийТоваровОбороты.Документ КАК Документ,
			|	ВЫБОР
			|		КОГДА ДвиженияСерийТоваровОбороты.Документ ССЫЛКА Документ.РеализацияТоваровУслуг
			|			ТОГДА ВЫРАЗИТЬ(ДвиженияСерийТоваровОбороты.Документ КАК Документ.РеализацияТоваровУслуг).Дата
			|		КОГДА ДвиженияСерийТоваровОбороты.Документ ССЫЛКА Документ.ПриобретениеТоваровУслуг
			|			ТОГДА ВЫРАЗИТЬ(ДвиженияСерийТоваровОбороты.Документ КАК Документ.ПриобретениеТоваровУслуг).Дата
			|		ИНАЧЕ ДАТАВРЕМЯ(1, 1, 1)
			|	КОНЕЦ КАК Период,
			|	ДвиженияСерийТоваровОбороты.КоличествоОборот КАК КоличествоОборот
			|ИЗ
			|	РегистрНакопления.ДвиженияСерийТоваров.Обороты(,, Запись, Номенклатура = &Номенклатура
			|	И (Документ ССЫЛКА Документ.РеализацияТоваровУслуг
			|	ИЛИ Документ ССЫЛКА Документ.ПриобретениеТоваровУслуг)
			|	И (Документ.Организация = &ОрганизацияДляПоискаРеализацийФ1 ИЛИ Документ.Организация = &ОрганизацияДляПоискаРеализацийФ2)
			|	И (Серия.ГоденДо = &ГоденДо
			|	ИЛИ &ГоденДо = ДАТАВРЕМЯ(1, 1, 1))) КАК ДвиженияСерийТоваровОбороты
			|УПОРЯДОЧИТЬ ПО
			|	Приоритет,
			|	Период";
			
	КонецЕсли;
		
	
	Запрос.УстановитьПараметр("Получатель", 	ПартнерДляДокумента);
	Запрос.УстановитьПараметр("ОрганизацияДляПоискаРеализаций", ОрганизацияДляПоискаРеализаций);
	Запрос.УстановитьПараметр("ОрганизацияДляПоискаРеализацийФ1", Шлюз.Организация);
	Запрос.УстановитьПараметр("ОрганизацияДляПоискаРеализацийФ2", Шлюз.ОрганизацияФ2);
	Запрос.УстановитьПараметр("ГоденДо", 		?(СрокГодностиДатой = Неопределено, '00010101', СрокГодностиДатой));
	Запрос.УстановитьПараметр("Номенклатура", 	Номенклатура);
	Результат = Запрос.Выполнить();
	
	ТЗ = Результат.Выгрузить();
	
	Возврат ТЗ;
	

КонецФункции

Функция ВернутьСрокГодностиИзВозврата(LineNumber, НомерЗаказа, ДатаЗаказа, СписокПримечаний)
	
	Результат = '00010101';
	ИндексНайденнойСтроки = 0;
	Пока ИндексНайденнойСтроки <= СписокПримечаний.Вграница() Цикл
		
		Если СписокПримечаний[ИндексНайденнойСтроки].OptionTypeId = "ExpirationDate" 
			И СписокПримечаний[ИндексНайденнойСтроки].CRMOrderDate = ДатаЗаказа 
			И СписокПримечаний[ИндексНайденнойСтроки].CRMOrderNumber =  НомерЗаказа 
			И СписокПримечаний[ИндексНайденнойСтроки].LineNumber = LineNumber Тогда 
			Результат = СписокПримечаний[ИндексНайденнойСтроки].Value;	
			Возврат Результат;
		КонецЕсли;
		
		ИндексНайденнойСтроки = ИндексНайденнойСтроки + 1;
	КонецЦикла;
	
	Возврат Результат;
	
КонецФункции

Функция ВернутьНоменклатуруИзРегистраСопоставления(СтруктураРеквизитовДокумента, НоменклатураМонолит, УпаковкаМонолит)
	
	ЗапросНоменклатуры 	= Новый Запрос;
	СтруктураОтвета 	= Новый Структура;
	
	СтруктураОтвета.Вставить("Номенклатура", 	Справочники.Номенклатура.ПустаяСсылка());
	СтруктураОтвета.Вставить("Упаковка", 		Справочники.УпаковкиЕдиницыИзмерения.ПустаяСсылка());
	СтруктураОтвета.Вставить("Коэффициент", 	1);
					
	ЗапросНоменклатуры.Текст = Mon_Общий.ВернутьТекстЗапросаНаПолучениеНоменклатуры();
	
	ЗапросНоменклатуры.УстановитьПараметр("НоменклатураМонолит", 	НоменклатураМонолит);
	ЗапросНоменклатуры.УстановитьПараметр("УпаковкаМонолит", 		УпаковкаМонолит);
	ЗапросНоменклатуры.УстановитьПараметр("ТипМонолита", 			СтруктураРеквизитовДокумента.ШлюзОрганизации.ТипМонолита);
	
	Результат = ЗапросНоменклатуры.Выполнить();
	
	Если Результат.Пустой() Тогда
		Возврат СтруктураОтвета;
	КонецЕсли;
	
	ВыборкаНоменклатуры = Результат.Выбрать();	
	ВыборкаНоменклатуры.Следующий();
	ЗаполнитьЗначенияСвойств(СтруктураОтвета, ВыборкаНоменклатуры);
			
	Возврат СтруктураОтвета;
	
КонецФункции

